s';\nimport Register from './Register.js';\nimport MyProfile from './MyProfile.js';\nimport ProtectedRoute from './ProtectedRoute';\nimport * as auth from '../auth.js';\n\nconst App = () => {\n  const [loggedIn, setLoggedIn] = useState(false);\n  const [userData, setUserData] = useState('');\n  const history = useHistory();\n\n  const handleLogin = ({ email, password }) => {\n    return auth.authorize(email, password)\n      .then((data) => {\n        if (data.token) {\n          localStorage.setItem('jwt', data.token);\n          tokenCheck();\n        }\n      })\n  }\n\n  const handleRegister = ({email, password }) => {\n    return auth.register(email, password)\n    .then(() => {\n      history.push('/signin');\n    });\n  }\n\n  const tokenCheck = () => {\n    if (localStorage.getItem('jwt')){\n      let jwt = localStorage.getItem('jwt');\n      auth.getContent(jwt)\n        .then((res) => {\n          setLoggedIn(true);\n          setUserData(res.data.email);\n        })\n        .then(() => {\n          history.push('/');\n        });\n      };\n    }\n\n  const signOut = () => {\n    localStorage.removeItem('jwt');\n    setLoggedIn(false);\n    setUserData(null);\n    history.push('/signup');\n    }\n\n  useEffect(() => {\n    tokenCheck();\n  }, []);\n\n  useEffect(() => {\n      if (loggedIn) {\n        history.push('/my-profile');\n      }\n  }, [loggedIn, history]);\n\n  return (\n      <Switch>\n        <ProtectedRoute path=\"/my-profile\" loggedIn={loggedIn}>\n            <MyProfile userData={userData} loggedIn={loggedIn} signOut={signOut}/>\n        </ProtectedRoute>\n        <Route path=\"/signin\">\n            <Login handleLogin={handleLogin} tokenCheck={tokenCheck} loggedIn={loggedIn}/>\n        </Route>\n        <Route path=\"/signup\">\n          <div className=\"registerContainer\">\n            <Register handleRegister={handleRegister} />\n          </div>\n        </Route>\n        <Route >\n          {loggedIn ? <Redirect to=\"/my-profile\" /> : <Redirect to=\"/signin\" />}\n        </Route>\n      </Switch>\n  );\n}\n\nexport default App;","/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Main.js",[],[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/ImagePopup.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Header.js",[],[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/ConfirmDeletePopup.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/EditAvatarPopup.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/EditProfilePopup.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Footer.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/AddPlacePopup.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/contexts/CurrentUserContext.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/utils/Api.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Card.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/PopupWithForm.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/auth.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/MyProfile.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Login.js",["202"],[],"import React, { useState } from 'react';\nimport Header from './Header';\n\nconst Login = (props) => {\n  const [formParams, setFormParams] = useState({\n    email: '',\n    password: '',\n  });\n  const [message, setMessage] = useState('');\n\n  const handleChange = (e) => {\n    const {name, value} = e.target;\n    setFormParams((prev) => ({\n      ...prev,\n      [name]: value\n    }));\n  }\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    let { email, password} = formParams;\n    props.handleLogin({ email, password })\n    .catch(err => {\n      setMessage(err.message);\n    });\n  }\n\n  return(\n    <div className=\"page\">\n      <Header loggedIn={props.loggedIn} navText='Регистрация' navLink='signup'/>\n        <div className=\"register\">\n          <h2 className=\"name name_place_register\">\n            Вход\n          </h2>\n          <form onSubmit={handleSubmit} className=\"register__form\">\n            <input id=\"email\" \n                  name=\"email\" \n                  type=\"email\" \n                  value={formParams.email} \n                  onChange={handleChange} \n                  className=\"popup__input popup__input_place-register\"\n                  placeholder=\"Email\"  \n                  required\n                  />\n            <input id=\"password\"  \n                    name=\"password\" \n                    type=\"password\" \n                    value={formParams.password} \n                    onChange={handleChange} \n                    className=\"popup__input popup__input_place-register\"\n                    placeholder=\"Пароль\"  \n                    required/>\n            <div className=\"register__button-container\">\n              <button type=\"submit\" \n                      className=\"popup__submit popup__submit_place-register\">\n                      Войти\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n  );\n}\n\nexport default Login;","/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/Register.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/ProtectedRoute.js",[],[],"/Users/Boris/Desktop/Praktikum/react-mesto-auth/src/components/InfoTooltip.js",[],[],{"ruleId":"203","severity":1,"message":"204","line":6,"column":7,"nodeType":"205","endLine":6,"endColumn":44},{"ruleId":"206","severity":1,"message":"207","line":54,"column":6,"nodeType":"208","endLine":54,"endColumn":8,"suggestions":"209"},{"ruleId":"210","severity":1,"message":"211","line":9,"column":10,"nodeType":"212","messageId":"213","endLine":9,"endColumn":17},"jsx-